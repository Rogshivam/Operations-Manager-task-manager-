{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)", 
      "dest": "https://YOUR-ACTUAL-BACKEND-URL.vercel.app/api/$1"  
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
